package painting;
import java.awt.EventQueue;

/**
* Runs SnapShop by instantiating and starting the SnapShopGUI.
* 
* class does have the main method.
* @author Ahana Ghosh
* @version Autumn 2015
*/
public final class ComponentsMain {
    
    /**
     * Private constructor, to prevent instantiation of this class.
     */
    private ComponentsMain() {
        throw new IllegalStateException();
    }
    
    /**
     * The main method, invokes the SnapShop GUI. Command line arguments are
     * ignored.
     * 
     * @param theArgs Command line arguments.
     */
    public static void main(final String[] theArgs) {
        EventQueue.invokeLater(new Runnable() {
            @Override
            public void run() {
                new Components().start();
            }
        });
    }
}
